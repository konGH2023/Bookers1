<h1>Editing Book</h1>

<%= form_with model: @book, url: 'books', method: :patch do |f| %>
<!-- %= form_with model: Book.new do |f| % -->
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<%= link_to "Show", book_path(@book) %>
<%= link_to "Back", books_path %>
<% end %>